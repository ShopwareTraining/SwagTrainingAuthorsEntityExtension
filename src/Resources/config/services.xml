<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="SwagTraining\AuthorsEntityExtension\Subscriber\AuthorEntityExtensionSubscriber">
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="SwagTraining\AuthorsEntityExtension\Command\AuthorEntityExtensionCommand">
            <argument type="service" id="author.repository" />
            <tag name="console.command"/>
        </service>
    </services>
</container>
